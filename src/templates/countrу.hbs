<div class="card">
  <div class="card-body">
    <h2 class="card-title">Capital: <span>{{capital}}</span></h2>
    <p class="card-text"><span>Population:</span> {{population}}</p>
    <p class="card-text"><span>Languages:</span> </p>

    <ul class="list-group"></ul>
    {{#each languages}}
    <li class="list">{{name}}</li>
    {{/each}}
    </ul>
  </div>
  <div class="card-img">
    <img src="{{flag}}" alt="flag">
  </div>
</div>